
<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for sda10d:
   - 7 joints

  Usage:
    robot_interface_streaming_sda10d.launch robot_ip:=<value> controller:=<fs100|dx100>
-->
<launch>
  <arg name="robot_ip" />

  <!-- controller: Controller name (fs100 or dx100) -->
  <arg name="controller"/>

  <rosparam command="load" file="$(find motoman_sda10d_support)/config/sda10d_motion_interface.yaml" />

  <include file="$(find motoman_driver)/launch/robot_multigroup_interface_streaming_$(arg controller).launch">
    <arg name="robot_ip"   value="$(arg robot_ip)" />
	</include>
</launch>
